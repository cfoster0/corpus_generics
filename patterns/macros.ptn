@ WORD          /^'{0,1}[a-zA-Z]+.*/;
@ TERMINAL      * !< *;
@ NP            /^NP/;
@ VP            /^VP/;
@ PP            /^PP/;
@ STRICTBP    (NNS @>> (/^NP/ << of | << /^PRP/ | << DT | << JJS)) . (VP < VBP);
@ BAREPLURAL    (NNS @>> (/^NP/ < /^PRP/ | < DT)) . (VP < VBP);
@ BPCONTEXT /^TOP/ << @BAREPLURAL;
@ INTERRUPT /UH/ > @BAREPLURAL;
